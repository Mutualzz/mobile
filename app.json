{
    "expo": {
        "name": "Mutualzz",
        "slug": "app",
        "version": "2.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "scheme": "com.mutualzz.app",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },

        "ios": {
            "bundleIdentifier": "com.mutualzz.app",
            "supportsTablet": true
        },
        "android": {
            "package": "com.mutualzz.app",
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "predictiveBackGestureEnabled": false
        },
        "experiments": {
            "reactCanary": true
        },
        "plugins": ["expo-router"],
        "extra": {
            "router": {},
            "eas": {
                "projectId": "348ab39b-4be0-4404-818c-674d6af866f0"
            }
        },
        "owner": "mutualzz"
    }
}
